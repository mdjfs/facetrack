{
  "name": "facetrack",
  "version": "0.0.1",
  "description": "Desktop app for security camera systems to track faces",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.mdjfs.facetrack",
    "productName": "Face Track",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": false,
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico"
    },
    "buildVersion": "0",
    "extraFiles": [{
      "from": "./package-bad-copy-but-easy-fix.json",
      "to": "./package.json"
    }]
  },
  "author": "Marcos Fuenmayor",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@tensorflow/tfjs-node": "^1.7.0",
    "@types/react-image-gallery": "^1.0.2",
    "@types/react-select": "^4.0.13",
    "@types/sharp": "^0.27.2",
    "cancelable-promise": "^3.2.3",
    "canvas": "^2.7.0",
    "electron-store": "^7.0.2",
    "face-api.js": "^0.22.2",
    "i18next": "^19.9.2",
    "jimp": "^0.16.1",
    "json-loader": "^0.5.7",
    "node-loader": "^1.0.2",
    "node-onvif": "^0.1.7",
    "node-onvif-ts": "^0.1.7",
    "onvif": "^0.6.4",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "spectron": "^13.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdjfs/facetrack"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack",
    "face-api",
    "face",
    "track",
    "security",
    "camera",
    "onvif"
  ],
  "bugs": {
    "url": "https://github.com/mdjfs/facetrack/issues"
  },
  "homepage": "https://github.com/mdjfs/facetrack#readme"
}